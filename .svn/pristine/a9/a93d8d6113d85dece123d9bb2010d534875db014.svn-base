import React,{useState,useEffect} from "react";
// @material-ui/core components
import { makeStyles } from "@material-ui/core/styles";
// core components

import CustomSelect from "components/CustomInput/CustomSelect.js";
import GridItem from "components/Grid/GridItem.js";
//import Table from "components/Table/Table.js";
import Card from "components/Card/Card.js";
import CardHeader from "components/Card/CardHeader.js";
import CardBody from "components/Card/CardBody.js";
import Icon from "@material-ui/core/Icon";
import CardIcon from "components/Card/CardIcon.js";
import Button from "components/CustomButtons/Button.js";
import Grid from '@material-ui/core/Grid';
import BackupIcon from "@material-ui/icons/Backup";
import StarIcon from "@material-ui/icons/Stars";
import CalendarBox from "components/CustomInput/CustomCalendar.js";
import SearchButton from "components/CustomButtons/Button.js";
import Popover from  '@material-ui/core/Popover';
import Excel from "views/Pages/BLUpload/ExcelUpload.js";
import AddCntrNum from "views/Pages/BLUpload/CntrNumRegist.js"
import CustomInput from "components/CustomInput/CustomInput.js";
import Autocomplete from '@material-ui/lab/Autocomplete';
import TextField from '@material-ui/core/TextField';
//import Modal from '@material-ui/core/Modal';
//import JoinPage from "components/Form/Common/JoinPage.js";
import axios from 'axios';
import MaterialTable,{MTableBodyRow, MTableCell} from 'material-table';
import leftPad from "left-pad";
// import page
import CarrierPage from "views/Pages/BLUpload/CarrierInfoPage.js";
import { Tooltip } from "@material-ui/core";
const styles = {
  cardTitleBlack: {
	    textAlign: "left",
	    color: "#000000",
	    minHeight: "auto",
	    fontWeight: "300",
	    fontFamily: "'Roboto', 'Helvetica', 'Arial', sans-serif",
	    marginBottom: "3px",
	    textDecoration: "none",
	    "& small": {
	      color: "#777",
	      fontSize: "65%",
	      fontWeight: "400",
	      lineHeight: "1"
	    }
	  },
    gridStyle1: {
    	paddingTop:'1px',
    	paddingBottom:'1px',
    },
};

const useStyles = makeStyles(styles);

export default function TableList(props) {
	 
	const classes = useStyles();
	const setEndDate = new Date();
	const [fromDate,setFromDate] = useState(new Date(setEndDate.setDate(setEndDate.getDate()-7)));
	const [toDate,setToDate] = useState(new Date());
	const [ieGubun, setIeGubun] = useState('');
	const [blbkgGubun, setBlbkgGubun] = useState('B/L');
	const [lineCode,setLineCode] = useState([]);
	const [labelName,setLabelName] = useState("B/L NO.");
	const [searchKey,setSearchKey] = useState("");
	const [searchCount, setSearchCount] = useState(0);
	const [store,setStore] = useState(props.store);
	useEffect(() => {
		axios.post("/loc/getCustomLineCode",{},{headers:{'Authorization':'Bearer '+store.token}}
		).then(res => setLineCode(res.data));
		// .then(res => console.log(JSON.stringify(res.data)));
		return () => {
			console.log('LINE CODE cleanup');
		};
	}, []);

	const handleIEGubun = (e) => {
		console.log("xx",e.target.value);
		setIeGubun(e.target.value);
		//e.target.value=="IMPORT"?setLabelName("BL No."):setLabelName("BK No.")
		
	}
	const handleblbkgGubun = (e) => {
		setBlbkgGubun(e.target.value);
		e.target.value==="B/L"?setLabelName("B/L No."):setLabelName("B/K No.")
		
	}	
	
	const [selectData,setSelectData] = useState([]);

	const [carrierCode,setCarrierCode] = useState("");
	const [anchorE, setAnchorE] = useState(null);
	const [anchorU, setAnchorU] = useState(null);
	const [anchorC, setAnchorC] = useState(null);
	const [openJoin, setOpenJoin] = useState(false);
	const [rowBLNO, setRowBLNO] = useState("");
	const [cntrList, setCntrList] = useState([]);
	const handleOpenJoin = () => {
		setOpenJoin(true);
	};
  
	const handleJoinClose = () => {
		setOpenJoin(false);
	};
  
	const handleClose = () => {
		setAnchorE(null);
		setAnchorU(null);
		setAnchorC(null);
	};
	const cntrNumbers = (returnFunction) => {
		console.log('returnFunction',returnFunction)
		setCntrList([]);
		setCntrList(returnFunction);
		handleClose();
	};
	const carrier_open = Boolean(anchorE);
	const upload_open = Boolean(anchorU);
	const cntrNum_add = Boolean(anchorC);
	const carrier = carrier_open ? 'simple-popover1':undefined;
	const upload = upload_open ? 'simple-popover2':undefined;
	const cntrNum = cntrNum_add ? 'simple-popover3':undefined;
	


	
	const onCarrierChange = (e,data) => {
		if(data) {setCarrierCode(data.id);} else {setCarrierCode("");}
	}


	const onSubmit = () => {

		let fromYMD = fromDate.getFullYear()+leftPad((fromDate.getMonth()+1), 2, "0")+leftPad((fromDate.getDate()), 2, "0");
		let toYMD = toDate.getFullYear()+leftPad((toDate.getMonth()+1), 2, "0")+leftPad((toDate.getDate()), 2, "0");
		let typeGubun = "";
		
		if (ieGubun === "IMPORT") {
			typeGubun = "I";
		}else if (ieGubun === "EXPORT") {
			typeGubun = "E";
		}else {
			typeGubun = "";
		}
		if( fromYMD > toYMD ) {
			alert( "종료일자가 시작 일자보다 빠릅니다. 다시 확인하세요." );
			return false;
		}

		axios.post("/loc/getMyBlList",{ carrierCode:carrierCode, fromDate:fromYMD, toDate:toYMD, typeGubun:typeGubun, blbkgGubun:blbkgGubun,searchKey:searchKey},{headers:{'Authorization':'Bearer '+store.token}})
		.then(res => (customSelectData(res.data)));
	};

	const customSelectData= (res) => {
		setSearchCount(res.length);
		let customData = [];
		res.map((prop,index) => {
			customData.push({carrier_code:<Tooltip title={prop.nm_kor} arrow><span value={prop.carrier_code}>{prop.carrier_code}</span></Tooltip>,
							 num: prop.num,
							 ie_type: prop.ie_type,
							 bkg_no: prop.bkg_no,
							 bl_no: prop.bl_no,
							 insert_date:prop.insert_date,
							 cntr_no: prop.cntr_no})
		});
		setSelectData(customData);
	}


	return (
		<div>
			<Card style={{marginBottom:'1px'}}>
				<CardHeader color="info" stats icon >
					<CardIcon color="info" style={{height:'55px'}}>
						<Icon style={{width:'26px',fontSize:'20px',lineHeight:'26px'}}>content_copy</Icon>
					</CardIcon>
					<h4 className={classes.cardTitleBlack}>B/L(B/K) MANAGEMENT</h4>
				</CardHeader>
				<CardBody style={{padding:'2px' ,marginBottom: '5px'}}>
					<GridItem>
						<Grid container spacing={2}>
							<Grid item xs={12} sm={12} md={2}>
								<CustomSelect
									id="ieGubun"
									labelText = "IMPORT&EXPORT"
									setValue = {ieGubun}
									option = {["ALL","IMPORT","EXPORT"]}
									inputProps={{onChange:handleIEGubun, authWidth:true}}
									
									formControlProps={{fullWidth: true}} 
								/>								
							</Grid>
							<Grid item xs={12} sm={12} md={1}>
								<CustomSelect
										id="blbkGubun"
										labelText = "B/L&B/K"
										setValue = {blbkgGubun}
										option = {["B/L","B/K"]}
										inputProps={{onChange:handleblbkgGubun}}
										formControlProps={{fullWidth: true}}/>
							</Grid>
								<Grid item xs={12} sm={12} md={2}>
									<TextField id="blbk" label={labelName} onChange={event => setSearchKey(event.target.value)} value={searchKey} //variant="outlined" size="small" 
										fullWidth />
								</Grid>
							<Grid item  xs={12} sm={12} md={3}>
								<Autocomplete
									options = {lineCode}
									getOptionLabel = { option => option.id + '\n' +option.nm }
									id="carrierCode"
									onChange={onCarrierChange}
									renderInput={params => (
										<TextField inputProps={{maxLength:4}} {...params} label="Carrier" fullWidth />
									)}
								/>
							</Grid>
						</Grid>



						<Grid container spacing={2}>
							<Grid item xs={12} sm={12} md={3}>
							<CalendarBox
								labelText ="Add Date From"
								id="fromDate"
								format="yyyy-MM-dd"
								setValue={fromDate}
								autoOk={true}
								onChangeValue={date => setFromDate(date)}
								formControlProps={{fullWidth: true}} 
							/>
							</Grid>
							<Grid item xs={12} sm={12} md={3}>
							<CalendarBox
								labelText =" Add Date To"
									id="toDate"
									format="yyyy-MM-dd"
									setValue={toDate}
									autoOk={true}
									onChangeValue={date => setToDate(date)}
								formControlProps={{fullWidth: true}}
							/>
							</Grid>
							<Grid item xs={12} sm={12} md={3}></Grid>
							<Grid item xs={12} sm={12} md={3}>
								<Grid >
									<SearchButton color="info" fullWidth onClick={onSubmit} >Search</SearchButton>
								</Grid>
							</Grid>
						</Grid>
					</GridItem>
					
				</CardBody>
			</Card>
			<Grid item xs={12}>
				<MaterialTable
					style={{marginTop:'5px'}}
					totalCount={10}
					options={{actionsColumnIndex: -1}}
					columns={
						[{cellStyle: {textAlign:'left'}, headerStyle: {textAlign:'left'}, width: '5%' ,title: 'No', field: 'num', editable:'never' }
						,{cellStyle: {textAlign:'left'}, headerStyle: {textAlign:'left'}, width: '5%' ,title: 'IMPORT EXPORT', field: 'ie_type', lookup: {'I': 'IMPORT', 'E': 'EXPORT'}, editComponent: props => (
							<CustomSelect
									id="ieGubun"
									labelText = "IMPORT&EXPORT"
									option = {["IMPORT","EXPORT"]}
									style={{textAlign:'center'}}
									setValue ={props.value =="IMPORT"? "I":"E"}
									inputProps={{
										onChange:event => props.onChange(event.target.value=="IMPORT"? "I":"E")
									}}
									
									formControlProps={{fullWidth: true}} 
							/>
						)}
						,{cellStyle: {textAlign:'left'}, headerStyle: {textAlign:'left'}, width: '15%' ,title: 'CARRIER', field: 'carrier_code', editComponent: props => (
							
							
							<Autocomplete
									options = {lineCode}
									getOptionLabel = { option => option.id !=null?option.id + ' ' +option.nm:"" }
									id="carrier_code"
									onChange={(event,data) => data!=null?props.onChange(data):props.onChange("")}
									
									value={props.value != null?props.value:""}
									renderInput={params => (
										<TextField {...params} label="Carrier" fullWidth />
									)}/>
		
							)
						}	   
						,{cellStyle: { textAlign:'left'}, headerStyle: { textAlign:'left'}, width: '20%' ,title: 'B/L No.', field: 'bl_no', editComponent: (props) => (
							<CustomInput
								labelText={
									<span>
									B/L No. <small>(required)</small>
									</span>
								}
								id="blno"
								formControlProps={{
								fullWidth: true
								}}
								inputProps={{
									max:35,
									onChange:event => props.onChange(event.target.value),
									value:props.value
								}}
							/>
							)}
						,{cellStyle: {textAlign:'left'}, headerStyle: {textAlign:'left'}, width: '20%' ,title: 'B/K No.', field: 'bkg_no', editComponent: (props) => (
							<CustomInput
								labelText={
									<span>
									B/K No. <small>(required)</small>
									</span>
								}
								id="bkgno"
								formControlProps={{
								fullWidth: true
								}}
								inputProps={{
									max:35,
									onChange:event => props.onChange(event.target.value),
									value:props.value
								}}
							/>
							)}
						,{cellStyle: {textAlignLast:'left'},headerStyle: {textAlignLast:'left', rowSpan:'3'}, width: '20%', title: 'Container No.', field: 'cntr_no', editComponent: (props) => (
							<CustomInput
								id='cntrno'
								formControlProps={{
									fullWidth: true
								
								}}
								inputProps={{
									
									maxLength:5,
									onChange:event => props.onChange(event.target.value),
									
									value:props.value,
								}}
							/>
							)}
						,{cellStyle: {textAlignLast:'left'},headerStyle: {textAlignLast:'left'}, width: '10%', title: '', field: 'multiCntrNum', editable:'onAdd', editComponent: (props) => (
							<Button
								variant="contained"
								color="info"
								size="sm"
								onClick={e=>{setAnchorC(e.currentTarget);setRowBLNO(props.rowData.bl_no)}}>Add Container Numbers
							</Button>
							)}
						,{cellStyle: {textAlignLast:'left'},headerStyle: {textAlignLast:'left'}, width: '15%', title: 'Add Date', field: 'insert_date', editable:'never' }]
					}
					data={selectData}
					components={{
						Toolbar: props => (
								<GridItem>
									<Grid item xs={12} sm={12} md={12}>
										<Grid style={{paddingTop:'10px'}}>
											<Button
												variant="contained"
												color="info"
												size="sm"
												onClick={() => props.actions[0].onClick()}
												startIcon={<BackupIcon/>}>B/L ADD
											</Button>&nbsp;&nbsp;
											<Button
												variant="contained"
												color="info"
												size="sm"
												startIcon={<BackupIcon/>}
												onClick={e=>setAnchorU(e.currentTarget)}>Excel Upload
											</Button>&nbsp;&nbsp;
											<Button
												variant="contained"
												color="info"
												size="sm"
												startIcon={<StarIcon/>}
												onClick={e=>setAnchorE(e.currentTarget)}>Carrier Info
											</Button>
											<Popover
											
												id={upload}
												open={upload_open}
												anchorEl={anchorU}
												onClose={handleClose}
												anchorReference="anchorPosition"
												anchorPosition={{top:10,left:550}}
												anchorOrigin={{vertical:'bottom',horizontal:'center',}}
												transformOrigin={{vertical:'top',horizontal:'center',}}><Excel token={store}
																											   params={lineCode} />
											</Popover>
											
											<Popover
												id={carrier}
												open={carrier_open}
												anchorEl={anchorE}
												onClose={handleClose}
												anchorReference="anchorPosition"
												anchorPosition={{top:80,left:550}}
												anchorOrigin={{vertical:'bottom',horizontal:'center',}}
												transformOrigin={{vertical:'top',horizontal:'center',}}><CarrierPage token={store}/>
											</Popover>
											<Popover
												id={cntrNum}
												open={cntrNum_add}
												anchorEl={anchorC}
												onClose={handleClose}
												anchorReference="anchorPosition"
												anchorPosition={{top:80,left:550}}
												anchorOrigin={{vertical:'bottom',horizontal:'center',}}
												transformOrigin={{vertical:'top',horizontal:'center',}}>
												<AddCntrNum params={rowBLNO}
															returnFunction={(param) => cntrNumbers(param)}/>
											</Popover>
											
											
											<label  style={{float:'right', padding:'10px'}}> Total : {searchCount}</label>
										</Grid>
									</Grid>
								</GridItem>
								
						),
					}}
					editable={{
						onRowAdd: newData =>
							
							new Promise(resolve => {
								setTimeout(() => {
									const carrierCode = newData.carrier_code.id;
									newData.carrier_code = carrierCode;
									console.log(newData.ie_type,'newData.ie_type')
									if ( undefined === newData.ie_type || '' === newData.ie_type) {
										newData.ie_type = "I";
									}
									if ( undefined === newData.carrier_code || '' === newData.carrier_code ) {
										alert("선사코드는 필수 입니다.");
										return false;
									}
									if (( '' === newData.bl_no || undefined === newData.bl_no) && ('' === newData.bkg_no || undefined === newData.bkg_no)) {
										alert("BL 번호 또는 BK 번호를 입력해주세요.");
										return false;
									} 

									if ( 4 < newData.carrier_code.length  ) {
										alert("선사코드는 최대 4자리 입니다.");
										return false;
									}
									if ( 16 < newData.bl_no.length ) {
										alert("BL번호는 16자리가 최대 입니다.");
										return false;
									}
									console.log(cntrList,'cntrList')
									if( cntrList.length > 0) {
										console.log(cntrList,'cntrList')
										cntrList.map((element, index) => {
											
											let cntrNum = element[0].toString();
											axios.post("/loc/getPkMyBlList",{params:newData, cntrNumber: cntrNum},{headers:{'Authorization':'Bearer '+store.token}}).then(res => {
												
												if ( res.data == '' ) {
													axios.post("/loc/insertBlRequest",{newData:newData, cntrNumber: cntrNum},{headers:{'Authorization':'Bearer '+store.token}}).then(res => {
													let carrier_kname = "";
													lineCode.forEach(element => {
														if (element.id == newData.carrier_code) {
														carrier_kname = element.id;
														return;
														}
													});
													let data = [];
													let carrierCode = newData.carrier_code;
													newData.carrier_code = <Tooltip title={carrier_kname} arrow><span value={carrierCode} key={index} >{carrierCode}</span></Tooltip>
													
													if( 0 < selectData.length ) {
														data = selectData;
													}
														
														onSubmit();
														setSearchCount(data.length);	
													})
												}else {
													alert("["+newData.bl_bkg+"] 해당 BL 값은 이미 존재합니다. 등록 불가합니다.");
													// data[data.indexOf(newData)] = oldData;
													// return { ...prevState, data };
												}
											})
										})
									}else {
										console.log(cntrList,'cntrList')
										axios.post("/loc/getPkMyBlList",{params:newData},{headers:{'Authorization':'Bearer '+store.token}}).then(res => {
										
										if ( res.data == '' ) {
											axios.post("/loc/insertBlRequest",{newData:newData},{headers:{'Authorization':'Bearer '+store.token}}).then(res => {
											let carrier_kname = "";
											lineCode.forEach(element => {
												if (element.id == newData.carrier_code) {
												carrier_kname = element.id;
												return;
												}
											});
											let data = [];
											let carrierCode = newData.carrier_code;
											newData.carrier_code = <Tooltip title={carrier_kname} arrow><span value={carrierCode}>{carrierCode}</span></Tooltip>

											if( 0 < selectData.length ) {
												data = selectData;
											}
												onSubmit();

												setSearchCount(data.length);	
											})
										}else {
											alert("["+newData.bl_bkg+"] 해당 BL 값은 이미 존재합니다. 등록 불가합니다.");
											// data[data.indexOf(newData)] = oldData;
											// return { ...prevState, data };
										}
									});
									}

									setCntrList([]);
									resolve();
								}, 600);
						}),
						onRowUpdate: (newData, oldData) =>
							
							new Promise((resolve, reject) => {
								setTimeout(() => {
									if ( undefined === newData.ie_type || '' === newData.ie_type) {
										newData.ie_type = 'I';
									}
									if ( undefined === newData.carrier_code || '' === newData.carrier_code ) {
										alert("선사코드는 필수 입니다.");
										return false;
									}
									if (( '' === newData.bl_no || undefined === newData.bl_no) && ('' === newData.bkg_no || undefined === newData.bkg_no)) {
										alert("BL 번호 또는 BK 번호를 입력해주세요.");
										return false;
									} 

									if ( 4 < newData.carrier_code.length  ) {
										alert("선사코드는 최대 4자리 입니다.");
										return false;
									}
									if ( 16 < newData.bl_no.length ) {
										alert("BL번호는 16자리가 최대 입니다.");
										return false;
									}
									if (oldData) {
										axios.post("/loc/getPkMyUpdateBlList",{params:newData},{headers:{'Authorization':'Bearer '+store.token}}).then(res => {
											console.log('res.data',res.data);
											// 수정 처리.
											if ( res.data == '' ) {

												axios.post("/loc/updateMyBlNo",{ oldData:oldData, newData:newData},{headers:{'Authorization':'Bearer '+store.token}});
												let data = selectData;
												const index = data.indexOf(oldData);
												data[index] = newData;
												setSelectData({data}, () => resolve());
											} else {
												console.log("event 1");
												alert("["+newData.bl_no+"] 해당 BL 값은 이미 존재합니다. 변경이 불가합니다.");
												resolve();
												// data[data.indexOf(newData)] = oldData;
												// return { ...prevState, data };
											}
										});
									}resolve();
									
								}, 600);
							}),
						onRowDelete: oldData =>
							new Promise(resolve => {
								setTimeout(() => {
									{
										
										
										// 삭제 처리.
										axios.post("/loc/deleteMyBlNo",{ oldData:oldData},{headers:{'Authorization':'Bearer '+store.token}})
										
										let data = selectData;
										const index = data.indexOf(oldData);
										data.splice(index, 1);
										setSelectData(data);
										setSearchCount(data.length);
									}
									resolve();
								}, 600);
						}),
					}}
				/>
			</Grid>
		</div>   
  );
  
}